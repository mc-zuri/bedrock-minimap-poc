{
  "name": "@minecraft-bedrock-minimap/electron-app",
  "version": "1.0.0",
  "type": "module",
  "description": "Electron desktop app for Minecraft Bedrock minimap",
  "author": "Minecraft Bedrock Minimap Team",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron .",
    "dev:overlay": "WEB_URL=http://localhost:3000 electron .",
    "build": "echo 'Electron app ready for packaging'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:win": "electron-forge make --platform win32",
    "make:mac": "electron-forge make --platform darwin",
    "make:linux": "electron-forge make --platform linux",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "electron-store": "^11.0.2",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "electron": "28.3.3"
  }
}
